{
    "compilerOptions": {
      "module": "commonjs",
      "outDir": "./dist",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "target": "ES2020",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "paths":{
        "*": ["node_modules/*"]
      },
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }